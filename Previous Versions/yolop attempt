    "\n",
    "#     def _get_input_sizes_dict(self):\n",
    "#         \"\"\"\n",
    "#         Build a dict {stream_name: feature_size}.\n",
    "#         \"\"\"\n",
    "#         input_sizes = {}\n",
    "#         special_cases = {\n",
    "#             \"TRAFFIC_LIGHT\": \"TL_STATE\",\n",
    "#             \"STATIC_CONTEXT\": \"STATIC\",\n",
    "#             \"EGO_SPEED\": \"EGO_SPEED\",\n",
    "#             \"EGO_ACC\": \"EGO_ACC\",\n",
