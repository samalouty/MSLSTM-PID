    "#         allg.extend(labs.cpu().numpy())\n",
    "#     return total / max(1, len(loader)), accuracy_score(allg, allp)\n",
    "\n",
    "\n",
    "# def evaluate_epoch(model, loader, crit, device):\n",
    "#     model.eval()\n",
    "#     total, allg, allp, allpr = 0.0, [], [], []\n",
